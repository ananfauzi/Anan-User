<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="login.content">
            <block class="Magento\Backend\Block\Template"
                name="anan_user_auth_login_buttons"
                template="Anan_User::login/buttons.phtml"
                after="admin.login"
            >
                <arguments>
                    <argument name="login_view_model" xsi:type="object">Anan\User\ViewModel\Adminhtml\Login</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>